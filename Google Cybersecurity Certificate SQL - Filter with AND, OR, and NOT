#This project is a part of the Google Cybersecurity Certificate#
#Goal 1 - Retrieve after hours failed attempts#
SELECT*
FROM log_in_attempts
WHERE login_time > '18:00' AND success = 'TRUE';
#Answer - 19#
#Goal 2 - Retrieve login attempts on specific dates#
SELECT*
FROM log_in_attempts
WHERE login_date = '2022-05-09' OR login_date = '2022-05-08';
#Answer - 75#
#Goal 3 - Retrieve login attempts outside of Mexico#
SELECT*
FROM log_in_attempts
WHERE NOT country LIKE 'MEX%';
#Answer - 144#
#Goal 4 - Retrieve employees in Marketing#
SELECT*
FROM employees
WHERE department = 'Marketing' AND office LIKE 'East%';
#Answer - elarson#
#Goal 5 - Retrieve employees in Finance or Sales#
SELECT*
FROM employees
WHERE department = 'Sales' OR department = 'Finance';
#Answer - lrodiqu#
#Goal 6 - Retrieve all employees not in IT#
SELECT*
FROM employees
WHERE NOT department = 'Information Technology';
#Answer - 161#
